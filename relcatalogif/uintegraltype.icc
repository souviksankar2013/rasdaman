inline r_Long *
UIntegralType::convertToCLong(const char *cell, r_Long *value) const
{
    return (r_Long *)(this->convertToCULong(cell, (r_ULong *)value));
}

inline double *
UIntegralType::convertToCDouble(const char *cell, r_Double *value) const
{
    r_Long tmp;
    *value = *this->convertToCLong(cell, &tmp);
    return value;
}

inline char *
UIntegralType::makeFromCLong(char *cell, const r_Long *value) const
{
    return this->makeFromCULong(cell, (const r_ULong *) value);
}

inline char *
UIntegralType::makeFromCDouble(char *cell, const r_Double *value) const
{
    r_Long tmp = *value;
    return this->makeFromCLong(cell, &tmp);
}
